{
  "name": "ts-express-boilerplate",
  "version": "1.1.0",
  "description": "Boilerplate for express and typescript",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint src/**/*.ts --fix --quiet",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "start": "node dist/index.js",
    "start-dev": "tsc && node dist/index.js",
    "migrate-dev": "tsc && typeorm migration:run --f config/ormconfig.json",
    "unmigrate-dev": "tsc && typeorm migration:revert --f config/ormconfig.json",
    "test": "jest --detectOpenHandles"
  },
  "author": "Bruno Salzano",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkstar/ts-express-boilerplate.git"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "ini": "^2.0.0",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "typeorm": "^0.2.29",
    "wait-for-expect": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.10",
    "@types/node": "^14.14.20",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "acorn": "^8.0.4",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "typeorm-model-generator": "^0.4.4",
    "typescript": "^4.1.3"
  }
}
